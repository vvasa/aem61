<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    width="500"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                <featuredTitle
                                jcr:primaryType="cq:Widget"
                                emptyText="Featured Title"
                                fieldLabel="Featured Title"
                                name="./featuredTitle"
                                width="500"
                                xtype="textfield"/>
                    <featuredetails
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Click + or â€“ to add child links to parent 1"
                        fieldLabel="Artist Details"
                        name="./featuredetails"
                        xtype="multicompositefield">
                        <fieldConfigs jcr:primaryType="cq:WidgetCollection">
                            <title
                                jcr:primaryType="cq:Widget"
                                emptyText="artistname"
                                fieldLabel="Artist name"
                                name="artistname"
                                width="500"
                                xtype="textfield"/>
                            <description
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Description"
                                name="description"
                                width="500"
                                xtype="textfield"/>
                            <imagepath
                                jcr:primaryType="cq:Widget"
                                emptyText="Image Path"
                                fieldLabel="ImagePath"
                                name="imagepath"
                                width="500"
                                xtype="pathfield"/>
                            <imagealttext
                                jcr:primaryType="cq:Widget"
                                emptyText="Image Alt Text"
                                fieldLabel="Alt Text"
                                name="imagealttext"
                                width="500"
                                xtype="textfield"/>
                            <linktext
                                jcr:primaryType="cq:Widget"
                                emptyText="Link Text"
                                fieldLabel="Link Text"
                                name="linktext"
                                width="500"
                                xtype="textfield"/>
                            <linkpath
                                jcr:primaryType="cq:Widget"
                                emptyText="Link Path"
                                fieldLabel="Link Path"
                                name="linkpath"
                                width="500"
                                xtype="pathfield"/>
                            <videopath
                                jcr:primaryType="cq:Widget"
                                emptyText="Video Path"
                                fieldLabel="Video Path"
                                name="videopath"
                                width="500"
                                xtype="pathfield"/>
                             <tooltip
                                jcr:primaryType="cq:Widget"
                                emptyText="Tooltip"
                                fieldLabel="Tooltip"
                                name="tooltip"
                                width="500"
                                xtype="textfield"/>
                        </fieldConfigs>
                    </featuredetails>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
