<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:lastModified="{Date}2015-12-11T15:44:44.517+05:30"
    jcr:lastModifiedBy="admin"
    jcr:mixinTypes="[mix:created,mix:lastModified,rep:AccessControllable]"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/projects/admin/card/projectcard"
    role_editor="projects-amextranslationfr-editor"
    role_observer="projects-amextranslationfr"
    role_owner="projects-amextranslationfr-owner">
    <jcr:content
        cq:template="/libs/cq/core/content/projects/templates/create-translation-project"
        jcr:description="Created for AMEX"
        jcr:primaryType="nt:unstructured"
        jcr:title="AMEX Translation FR"
        sling:resourceType="cq/gui/components/projects/admin/card/projectcontent"
        active="{Boolean}true"
        contentCategory="general"
        coverUrl="/content/dam/projects/amextranslationfr/cover"
        damFolderPath="/content/dam/projects/amextranslationfr"
        destinationLanguage="fr"
        detailsHref="/projects/details.html"
        sourceLanguage="en"
        translationCloudConfigName="Microsoft Translator Trial License"
        translationCloudConfigPath="/etc/cloudservices/msft-translation/msft_trial"
        translationMethod="MACHINE_TRANSLATION"
        translationProvider="microsoft">
        <links jcr:primaryType="nt:unstructured"/>
        <dashboard jcr:primaryType="nt:unstructured">
            <gadgets jcr:primaryType="nt:unstructured">
                <team
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Team"
                    sling:resourceType="cq/gui/components/projects/admin/pod/teampod"
                    cardWeight="60"/>
                <tasks
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Tasks"
                    sling:resourceType="cq/gui/components/projects/admin/pod/taskpod"
                    cardWeight="70"/>
                <translationjob
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Translation Job"
                    sling:resourceType="cq/gui/components/projects/admin/pod/translationjobpod"
                    assetTagTranslation="{Boolean}true"
                    cardWeight="90"
                    currentActionInProgress="{Boolean}false"
                    currentPollerAction="START_TRANSLATION"
                    pageTagTranslation="{Boolean}true"
                    translationStatus="READY_FOR_REVIEW">
                    <child_pages jcr:primaryType="nt:unstructured">
                        <france
                            jcr:primaryType="nt:unstructured"
                            jcr:title="France"
                            sling:resourceType="cq/gui/components/projects/admin/card/translation_object"
                            independentObject="{Boolean}true"
                            isEmbedded="{Boolean}false"
                            mimeType="text/html"
                            name="fr"
                            sourcePath="/content/amex/fr"
                            translationFileType="PAGE"
                            translationStatus="READY_FOR_REVIEW"/>
                        <homepage
                            jcr:primaryType="nt:unstructured"
                            jcr:title="homepage"
                            sling:resourceType="cq/gui/components/projects/admin/card/translation_object"
                            independentObject="{Boolean}true"
                            isEmbedded="{Boolean}false"
                            mimeType="text/html"
                            name="homepage"
                            sourcePath="/content/amex/fr/homepage"
                            translationFileType="PAGE"
                            translationStatus="READY_FOR_REVIEW"/>
                        <tagmetadata
                            jcr:primaryType="nt:unstructured"
                            jcr:title="TAGMETADATA"
                            sling:resourceType="cq/gui/components/projects/admin/card/translation_object"
                            independentObject="{Boolean}true"
                            mimeType="text/xml"
                            translationFileType="TAGMETADATA"
                            translationStatus="READY_FOR_REVIEW"/>
                    </child_pages>
                </translationjob>
                <translationprojectsummarypod
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Translation Project Summary"
                    sling:resourceType="cq/gui/components/projects/admin/pod/translationprojectsummarypod"
                    cardWeight="100"/>
            </gadgets>
        </dashboard>
        <projectThumbnail/>
    </jcr:content>
    <rep:policy/>
</jcr:root>
