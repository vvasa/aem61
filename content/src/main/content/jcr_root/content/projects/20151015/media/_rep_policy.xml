<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <deny
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:all]"/>
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="projects-media-photographers"
        rep:privileges="{Name}[jcr:addChildNodes,jcr:removeChildNodes,jcr:nodeTypeManagement,jcr:modifyProperties]"/>
    <allow0
        jcr:primaryType="rep:GrantACE"
        rep:principalName="projects-media-photographers"
        rep:privileges="{Name}[jcr:removeNode]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*"/>
    </allow0>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="projects-media-editor"
        rep:privileges="{Name}[jcr:addChildNodes,jcr:removeChildNodes,jcr:nodeTypeManagement,jcr:modifyProperties]"/>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="projects-media-editor"
        rep:privileges="{Name}[jcr:removeNode]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*"/>
    </allow2>
    <allow3
        jcr:primaryType="rep:GrantACE"
        rep:principalName="projects-media-copywriters"
        rep:privileges="{Name}[jcr:addChildNodes,jcr:removeChildNodes,jcr:nodeTypeManagement,jcr:modifyProperties]"/>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="projects-media-copywriters"
        rep:privileges="{Name}[jcr:removeNode]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*"/>
    </allow4>
    <allow5
        jcr:primaryType="rep:GrantACE"
        rep:principalName="projects-media-designers"
        rep:privileges="{Name}[jcr:addChildNodes,jcr:removeChildNodes,jcr:nodeTypeManagement,jcr:modifyProperties]"/>
    <allow6
        jcr:primaryType="rep:GrantACE"
        rep:principalName="projects-media-designers"
        rep:privileges="{Name}[jcr:removeNode]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*"/>
    </allow6>
    <allow7
        jcr:primaryType="rep:GrantACE"
        rep:principalName="projects-media-owner"
        rep:privileges="{Name}[jcr:all]"/>
    <allow8
        jcr:primaryType="rep:GrantACE"
        rep:principalName="projects-media"
        rep:privileges="{Name}[jcr:read,jcr:readAccessControl]"/>
</jcr:root>
