<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Widget"
                title="Explore Artist"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <artistname
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Artist Name"
                        name="./artistname"
                        xtype="textfield"/>
                    <image
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Artist Image"
                        name="./image"
                        xtype="pathfield"/>
                    <linktext
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Link Text"
                        name="./linktext"
                        xtype="textfield"/>
                    <linkpath
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Link Path"
                        name="./linkpath"
                        xtype="pathfield"/>
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title"
                        name="./title"
                        xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
