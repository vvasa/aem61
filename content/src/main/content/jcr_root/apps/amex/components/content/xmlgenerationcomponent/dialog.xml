<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Widget"
                title="XML Configurations"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title"
                        name="./title"
                        xtype="textfield"/>
                    <age
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Age"
                        name="./age"
                        xtype="textfield"/>
                    <name
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Name"
                        name="./name"
                        xtype="textfield"/>
                    <filePath
                        jcr:primaryType="cq:Widget"
                        fieldLabel="filePath"
                        fieldDescription="Enter the path of the Destination file Eg : C:\\temp\\FileNamefile.xml "
                        name="./filePath"
                        xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
